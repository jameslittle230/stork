{
  "name": "stork",
  "version": "0.6.0",
  "description": "Impossibly fast web search.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jameslittle230/stork.git"
  },
  "keywords": [
    "search",
